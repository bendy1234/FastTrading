  "authors": [
    "ADudeCalledLeo",
    "bendy1234"
  ],
  "contact": {
    "sources": "https://github.com/bendy1234/FastTrading",
    "issues": "https://github.com/bendy1234/FastTrading/issues"
  },
  "license": "MIT",
  "icon": "assets/speedtrading/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "adudecalledleo.speedtrading.SpeedTrading"
    ],
    "modmenu": [
      "adudecalledleo.speedtrading.compat.ModMenuSupport"
    ]
  },
  "mixins": [
    "speedtrading.mixins.json"
  ],
  "accessWidener": "fasttrading.accesswidener",
  "depends": {
    "fabricloader": ">=0.10.1+build.209",
